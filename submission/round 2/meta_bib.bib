journal = {Psychosomatic Medicine},
keywords = {adolescent,adult,asthma,asthma diagnosis,asthma psychology,asthma therapy,emotions,follow up studies,forced expiratory volume,humans,life change events,respiratory function tests,spirometry,treatment outcome,vital capacity,writing},
number = {1},
pages = {130--136},
pmid = {15673635},
title = {{Does writing affect asthma? A randomized trial.}},
