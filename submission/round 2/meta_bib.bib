primaryClass = {hep-ph},
title = {{The rules of the game called psychological science}},
url = {http://pps.sagepub.com/content/7/6/543.abstract{\%}5Cnhttp://pps.sagepub.com/content/7/6/543.full.pdf},
volume = {7},
year = {2012}
}
@incollection{Pennebaker1989,
abstract = {This chapter explores the nature of confession and inhibition. Conversely, not confiding significant experiences is associated with increased disease rates, ruminations, and other difficulties. This pattern of findings has helped in developing a useful theory of active inhibition that shares many of the assumptions of learning theory, psychodynamic models, and more recent cognitive perspectives. The chapter examines the nature of confession per se. The chapter focuses on the physiological and psychological effects of confronting or actively avoiding past traumatic experiences. Based on a number of laboratory and field studies, it is clear that requiring people to write or talk about traumas is associated with both immediate and long-term health benefits. The chapter presents a formal theory of active inhibition. The links among the theory and Freud, animal learning, and cognitive perspectives are discussed in the chapter. The chapter describes the reexamination of catharsis, the development and breakdown of the self, and the role of psychosomatics in social psychology.},
